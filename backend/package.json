{
    "name": "smartpark-backend",
    "version": "1.0.0",
    "description": "SmartPark Backend API",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "db:create": "npx sequelize-cli db:create",
        "db:drop": "npx sequelize-cli db:drop",
        "seed": "npx sequelize-cli db:drop && npx sequelize-cli db:create && node seed.js"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.2",
        "pg": "^8.11.3",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.32.1"
    },
    "devDependencies": {
        "nodemon": "^3.0.1",
        "sequelize-cli": "^6.6.1"
    }
}